<%= form_for(@group) do |f| %>
  <%= f.label(:name) %>
  <%= f.text_field(:name) %><br>


  <%= f.label(:addr) %>
  <%= f.text_field(:addr) %><br>

  <%= f.label(:addr2) %>
  <%= f.text_field(:addr2) %><br>

  <%= f.label(:city) %>
  <%= f.text_field(:city) %><br>

  <%= f.label(:state) %>
  <%= f.text_field(:state) %><br>

  <%= f.label(:zip) %>
  <%= f.text_field(:zip) %><br>

  <%= f.label(:phone) %>
  <%= f.text_field(:phone) %><br>

  <%= f.label(:lunch_time) %>
  <%= f.text_field(:lunch_time) %><br>

  <%= f.submit %>
<% end %>